{
  "name": "Solarized Dark (@pradyunsg)",
  "type": "dark",
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "enumMember": {
      "foreground": "#859900"
    },
    "variable.constant": {
      "foreground": "#6c71c4"
    },
    "variable.defaultLibrary": {
      "foreground": "#859900"
    }
  },
  "tokenColors": [
    // General
    {
      "name": "Comments",
      "scope": "comment",
      "settings": {
        "foreground": "#586e75"
      }
    },
    {
      "name": "Strings",
      "scope": "string",
      "settings": {
        "foreground": "#2aa198"
      }
    },
    {
      "name": "Support values",
      "scope": "support",
      "settings": {
        "foreground": "#6c71c4"
      }
    },
    {
      "name": "Names of things",
      "scope": "entity.name",
      "settings": {
        "foreground": "#268bd2"
      }
    },
    {
      "name": "Exception names",
      "scope": "support.type.exception",
      "settings": {
        "foreground": "#268bd2"
      }
    },
    {
      "name": "Constants",
      "scope": "constant",
      "settings": {
        "foreground": "#6c71c4"
      }
    },
    {
      "name": "Parameters",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#b58900"
      }
    },
    {
      "name": "Language Variables",
      "scope": "variable.language",
      "settings": {
        "foreground": "#6c71c4"
      }
    },
    {
      "name": "Storage Names (class, def, struct, function)",
      "scope": "storage.type",
      "settings": {
        "foreground": "#d33682"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "foreground": "#d33682"
      }
    },
    {
      "name": "Comment 'NOTE' tags",
      "scope": "comment keyword.codetag",
      "settings": {
        "fontStyle": "bold"
      }
    },
    // Markup
    {
      "scope": "markup.quote",
      "settings": {
        "foreground": "#cb4b16",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#b58900"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "foreground": "#b58900",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "foreground": "#b58900",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "punctuation.definition.quote, punctuation.definition.bold, punctuation.definition.italic",
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "name": "Heading symbols",
      "scope": "punctuation.definition.heading",
      "settings": {
        "foreground": "#d33682"
      }
    },
    {
      "name": "Link (brackets and all)",
      "scope": "meta.link",
      "settings": {
        "foreground": "#859900"
      }
    },
    {
      "name": "Link (destination)",
      "scope": "meta.link markup.underline.link",
      "settings": {
        "foreground": "#268bd2"
      }
    },
    // Python-specific
    {
      "name": "Python Function calls",
      "scope": "meta.function-call.generic.python",
      "settings": {
        "foreground": "#268bd2"
      }
    },
    // Web-specific
    {
      "name": "Normal Variables",
      "scope": "entity.name.variable",
      "settings": {
        "foreground": "#93a1a1"
      }
    },
    {
      "name": "Tag names",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#cb4b16"
      }
    },
    {
      "name": "Property names",
      "scope": "support.type.property-name",
      "settings": {
        "foreground": "#859900"
      }
    },
    {
      "name": "CSS functions",
      "scope": "source.css support.function",
      "settings": {
        "foreground": "#2aa198"
      }
    },
    {
      "name": "CSS classes",
      "scope": "source.css entity.other",
      "settings": {
        "foreground": "#268bd2"
      }
    },
    {
      "name": "CSS variables",
      "scope": "source.css variable",
      "settings": {
        "foreground": "#93a1a1"
      }
    },
  ],
